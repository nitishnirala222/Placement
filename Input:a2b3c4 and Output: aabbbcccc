//Using C-Programming...

#include<stdio.h>
#include<string.h>

int main() {
   char str[20];
    printf("Enter  String :  ");
    scanf("%s",&str);
    for(int i=0;i<strlen(str);i=i+2) {
        for(int j=0;j<(str[i]-48);j++) {
            printf("%c",str[i+1]);
        }

    }
}

// Using Cpp-Programming...

#include<iostream>
#include<string.h>
using namespace std;

int main() {
   char str[20];
    cout<<"Enter  String : ";
    cin>>str;
    for(int i=0;i<strlen(str);i=i+2) {
        for(int j=0;j<(str[i]-48);j++) {
            cout<<str[i+1];
        }

    }
}
